<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.serviceModel>
        <behaviors>
            <serviceBehaviors>
                <behavior name="Metadata">
                    <serviceMetadata httpGetEnabled="true" />
                </behavior>
            </serviceBehaviors>
        </behaviors>
        <services>
            <service behaviorConfiguration="Metadata" name="EvalServiceLibrary.EvalService">
                <clear />
                <endpoint address="basic" binding="basicHttpBinding" contract="EvalServiceLibrary.IEvalService"
                    listenUriMode="Explicit">
                </endpoint>
                <endpoint address="ws" binding="ws2007HttpBinding" contract="EvalServiceLibrary.IEvalService"
                    listenUriMode="Explicit">
                </endpoint>
                <endpoint address="net.tcp://localhost:8111/evalservicehost" binding="netTcpBinding"
                    contract="EvalServiceLibrary.IEvalService" listenUriMode="Explicit">
                </endpoint>
                <endpoint address="net.pipe://localhost/evalservicehost" binding="netNamedPipeBinding"
                    contract="EvalServiceLibrary.IEvalService" listenUriMode="Explicit">
                </endpoint>
                <endpoint address="mex" binding="basicHttpBinding" bindingConfiguration=""
                    contract="IMetadataExchange" />
                <host>
                    <baseAddresses>
                        <add baseAddress="http://localhost:8181/evalservicehost" />
                    </baseAddresses>
                </host>
            </service>
        </services>
    </system.serviceModel>
</configuration>